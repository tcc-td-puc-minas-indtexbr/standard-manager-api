{
  "version": "2.0",
  "app_name": "standard_manager_api",
  "tags": {
    "Sector": "IndTexBr",
    "Team": "TCC TD SIGO",
    "App": "Standard Manager API"
  },
  "lambda_timeout": 120,
  "memory": 128,
  "stages": {
    "production": {
      "manage_iam_role": false,
      "iam_role_arn": "arn:aws:iam::${AWS::AccountId}:role/lambda_role",
      "lambda_functions": {
        "handle_sqs_message": {
          "lambda_timeout": 120
        }
      },
      "environment_variables": {
        "APP_ENV": "production",
        "DEBUG": "false",
        "LOG_LEVEL": "info",
        "DYNAMODB_TABLE_NAME": "standard-manager-dynamodb-StandardTable-1OHDYTJP208R1"
      },
      "tags": {
        "Env": "production"
      }
    },
    "staging": {
      "manage_iam_role": false,
      "iam_role_arn": "arn:aws:iam::${AWS::AccountId}:role/lambda_role",
      "lambda_functions": {
        "handle_sqs_message": {
          "lambda_timeout": 120
        }
      },
      "environment_variables": {
        "APP_ENV": "staging",
        "DEBUG": "false",
        "LOG_LEVEL": "info",
        "DYNAMODB_TABLE_NAME": "standard-manager-dynamodb-StandardTable-1OHDYTJP208R1"
      },
      "tags": {
        "Env": "staging"
      }
    },
    "dev": {
      "manage_iam_role": false,
      "iam_role_arn": "arn:aws:iam::${AWS::AccountId}:role/lambda_role",
      "lambda_functions": {
        "handle_sqs_message": {
          "lambda_timeout": 120
        }
      },
      "environment_variables": {
        "APP_ENV": "development",
        "DEBUG": "true",
        "LOG_LEVEL": "info",
        "DYNAMODB_TABLE_NAME": "standard-manager-dynamodb-StandardTable-1OHDYTJP208R1",
        "AWS_PROFILE": "tcc-td-puc-minas-admin"
      }
    }
  }
}